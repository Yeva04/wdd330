{
  "name": "outside",
  "version": "2.0.0",
  "description": "Boilerplate for WDD 330 SleepOutside project",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build && copy redirect.html dist\\index.html",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint *.js src/**/*.js",
    "format": "prettier --ignore-path ./.gitignore --write \"./**/*.{html,json,js,ts,css}\"",
    "test": "jest"
  },
  "homepage": "https://yeva04.github.io/wdd330/",
  "author": "Shane Thompson",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "vite": "^7.1.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "dependencies": {
    "@vitejs/plugin-legacy": "^7.2.1"
  }
}
